<%- include('includes/head.ejs') %>
</head>
<body>
    <div class="container">
        <%- include('includes/navigation.ejs') %>
        <div class="content">
           <h1>View Reports</h1>
           <br>
           <% for(var reportNum=0; reportNum<reports.length; reportNum++) {%>
            <h2><%= reports[reportNum].title %></h2>

            <table class="table">
                <thead class="thead-dark">
                <tr>
                    <% for(var j=0; j<reports[reportNum].data[0].length; j++) {%>
                        <th><%= reports[reportNum].data[0][j] %></th>
                        <% } %>
                  </tr>
                </thead>
                  <% for(var i=1; i<reports[reportNum].data.length; i++) {%>
                    <tr>
                        <% for(var j=0; j<reports[reportNum].data[i].length; j++) {%>
                            <td><%= reports[reportNum].data[i][j] %></td>
                            <% } %>
                    </tr>
                   
                    <% } %>
            </table>

         <% } %>
       </div>
    </div>
    <%- include('includes/end.ejs') %>